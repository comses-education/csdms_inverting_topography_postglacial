  coliny_ea
    seed = 2414
    population_size = 23
  max_function_evaluations = 300
  final_solutions = 6
  output verbose
  model_pointer = 'M1'

method
  optpp_g_newton
    id_method = 'GN'
    convergence_tolerance = 1e-08
    search_method = gradient_based_line_search
    max_step = 1.11803398875
    gradient_tolerance = 1e-06
    model_pointer = 'M2'

method
  optpp_g_newton
    id_method = 'GN_final'
    convergence_tolerance = 1e-08
    search_method = gradient_based_line_search
    max_step = 1.11803398875
    gradient_tolerance = 1e-06
    model_pointer = 'M3'


model
