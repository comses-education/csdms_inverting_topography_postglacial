# Dakota input file
environment
  top_method_pointer = 'HS'
  tabular_data
  tabular_data_file = 'wv_model_104_calib.dat'

method
  id_method = 'HS'
  hybrid sequential
  method_pointer_list = 'GA' 'GN' 'GN_final'

method
